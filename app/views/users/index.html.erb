<h1>All Users</h1>

 <%= will_paginate %>

 <div id= "team members">
     <%list = User.all=%>
     <table class="table-style">
        <%list.each do |user| %>
    <% next if user == current_user %>
        <tr>
            <td><%=user.name%></td>
            <td><%=user.admin%></td>
            <td><%= button_to "Toggle Admin", {action: "make_admin", id: user.id }, method: :put %></td>
        </tr>
        <% end %>
     </table>
 </div>

 <%= will_paginate %>
